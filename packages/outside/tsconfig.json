{
  "extends": "@bitgouel/tsconfig/next.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@outside/*": ["./*"]
    },
    "strict": false,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
